<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.propertycross" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PropertyCross</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="19" />
        <allow-intent href="market:*" />
        <icon src="resources/icon/android/36x36.png" density="ldpi"/>
        <icon src="resources/icon/android/48x48.png" density="mdpi"/>
        <icon src="resources/icon/android/96x96.png" density="hdpi"/>
        <splash src="resources/screen/android/320x480.png" density="ldpi"/>
        <splash src="resources/screen/android/480x800.png" density="mdpi"/>
        <splash src="resources/screen/android/720x1280.png" density="xhdpi"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="Orientation" value="portrait" />
</widget>
